{
  "name": "peeler",
  "description": "parser for only bracket.",
  "version": "1.1.0",
  "author": {
    "name": "elzup",
    "email": "guild0105@gmail.com",
    "url": "elzup.com"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
